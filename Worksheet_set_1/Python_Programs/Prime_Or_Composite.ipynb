   "source": [
    "n = int(input(\"Enter a number \"))\n",
    "p=0\n",
    "for i in range(1,n+1):\n",
    "    if n%i ==0:\n",
    "        p=p+1  \n",
    "if n == 0 or n==1:\n",
    "    print('Neither a prime or composite number')\n",
    "elif p==2:\n",
    "    print('Prime number')\n",
    "else:\n",
    "    print('composite number')"
   ]
